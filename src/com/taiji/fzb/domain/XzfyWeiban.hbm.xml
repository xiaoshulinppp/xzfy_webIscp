<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping
	PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.taiji.fzb.domain">
<class name="XzfyWeiban" table="XZFY_WEIBAN">
	<id name="id" type="java.lang.Integer" column="id">
		<generator class="increment"/>
    </id>
    <property name="locbmdq" type="java.lang.String">
        <column name="LOCBMDQ" length="50" />
    </property>
    
    <property name="locbmwb" type="java.lang.String">
        <column name="LOCBMWB" length="50" />
    </property>
    
    <property name="orgname" type="java.lang.String">
       <column name="ORGNAME" length="100" />
    </property>
</class>
</hibernate-mapping>