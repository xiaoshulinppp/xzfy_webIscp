<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping package="com.taiji.ashjl.domain">
<class 
    name="AshjlInfo" 
    table="ASHJLINFO"
    dynamic-insert="false" 
    dynamic-update="false" 
>
    <id
        name="id"
        type="java.lang.String"
        column="id"
    >
        <generator class="assigned"/>
    </id>
   <property
        name="time"
        type="java.lang.String"
        column="time"
      
    />	

    <property
        name="location"
        type="java.lang.String"
        column="location"

    />
   <property
        name="host"
        type="java.lang.String"
        column="host"
 
    />	
    <property
        name="attendant"
        type="java.lang.String"
        column="attendant"
	
    />
    <property 
    	name="filePath"
    	type="java.lang.String"
    	column="filePath"
    />
    <property 
    	name="realFilePath"
    	type="java.lang.String"
    	column="realFilePath"
    />
    <property 
    	name="fileName"
    	type="java.lang.String"
    	column="fileName"
    />
    <property 
    	name="xzfyId"
    	type="java.lang.String"
    	column="xzfyId"
    />
    <property 
    	name="graded"
    	type="java.lang.String"
    	column="graded"
    />
 
			
</class>
</hibernate-mapping>
