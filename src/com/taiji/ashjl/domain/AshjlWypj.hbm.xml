<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping package="com.taiji.ashjl.domain">
<class 
    name="AshjlWypj" 
    table="AshjlWypj"
    dynamic-insert="false" 
    dynamic-update="false" 
>
    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="increment" />
    </id>
    <property
    	name="ashjlId"
    	type="java.lang.String"
    	column="ashjlId"
    />
    <property 
    	name="xzfyId"
    	type="java.lang.String"
    	column="xzfyId"
    />
    <property
    	name="committeeId"
    	type="java.lang.Integer"
    	column="committeeId"
    />
    <property 
    	name="committeeName"
    	type="java.lang.String"
    	column="committeeName"
    />
    <property
    	name="grade"
    	type="java.lang.String"
    	column="grade"
    />
    <property
    	name="isHost"
    	type="java.lang.String"
    	column="isHost"
    />
        
 
			
</class>
</hibernate-mapping>
