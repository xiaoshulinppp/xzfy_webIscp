<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping package="com.taiji.schedule.domain">
<class 
    name="ScheduleInfo" 
    table="ScheduleInfo">
	<id
		name="id" type="java.lang.Integer"
        column="ID"
    >
      <generator class="increment" />
    </id>
   	<property
        name="scheduleDate"
        type="java.lang.String"
        column="scheduleDate"
    />
   	
    <property
        name="title"
        type="java.lang.String"
        column="title"
    />
    <property
        name="content"
        type="java.lang.String"
        column="content"
    />


    <property
        name="recorder"
        type="java.lang.String"
        column="recorder"
    />
   <property name="recorderId"
   		type="java.lang.Integer"
   		column="recorderId"
   	/>

  

</class>
</hibernate-mapping>
