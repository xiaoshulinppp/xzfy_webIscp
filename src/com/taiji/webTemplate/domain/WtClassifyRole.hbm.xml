<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.taiji.webTemplate.domain.WtClassifyRole" table="WT_CLASSIFY_ROLE">
		<id name="id" type="java.lang.Integer">
            <column name="ID" />
            <generator class="increment" />
        </id>
        
        <property name="roleId" type="java.lang.Integer">
            <column name="WT_ROLE_ID" />
        </property>
        
        <property name="classifyId" type="java.lang.Integer">
            <column name="WT_CLASSIFY_ID" />
        </property>
	</class>
</hibernate-mapping>