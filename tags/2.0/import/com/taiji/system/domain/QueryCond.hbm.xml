<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping package="com.taiji.system.domain">
<class 
    name="QueryCond" 
    table="t_queryCond"
    dynamic-update="true"
	dynamic-insert="true"    
>
    <id
        name="id"
        type="java.lang.Integer"
        column="query_id"
    >
        <generator class="increment" />
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="query_name"
		not-null="true"
    />
	<property 
		name="userId"
		type="java.lang.Integer"
		column="query_userId"
        not-null="true"
	/>
	<property
        name="desc"
        type="java.lang.String"
        column="query_desc"
    />
    <one-to-one name="search" class="SearchStateCond" fetch="join" cascade="all"/>
</class>
</hibernate-mapping>