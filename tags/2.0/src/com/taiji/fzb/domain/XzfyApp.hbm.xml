<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping package="com.taiji.fzb.domain">
<class 
    name="XzfyApp" 
    table="xzfy_app"
    dynamic-update="true"
	dynamic-insert="true"    
>
    <id
        name="id"
        type="java.lang.Integer"
        column="id"
    >
        <generator class="increment" />
    </id>

   <property
        name="app_name"
        type="java.lang.String"
        column="app_name"
      
    />	

    <property
        name="app_id"
        type="java.lang.String"
        column="app_id"

    />
   <property
        name="isdeputy"
        type="java.lang.String"
        column="isdeputy"
 
    />	
   <property
        name="mobile"
        type="java.lang.String"
        column="mobile"
 
    />	
    <property
        name="email"
        type="java.lang.String"
        column="email"
	
    />

   <property
        name="address"
        type="java.lang.String"
        column="address"
 
    />	
   <property
        name="mail_address"
        type="java.lang.String"
        column="mail_address"
 
    />	
    <property
        name="sexy"
        type="java.lang.String"
        column="sexy"
	
    />

    <property
        name="proxytype"
        type="java.lang.String"
        column="proxytype"
	
    />
 
     <property
        name="postcode"
        type="java.lang.String"
        column="postcode"
	
    />
    
        <property
        name="telephone"
        type="java.lang.String"
        column="telephone"
	
    />
    
     <property
        name="islink"
        type="java.lang.String"
        column="islink"
	
    />
    
     <property
        name="deputyName"
        type="java.lang.String"
        column="deputy_name"
	
    />
    
     <property
        name="deputyType"
        type="java.lang.String"
        column="deputy_type"
	
    />
    
     <property
        name="otherType"
        type="java.lang.String"
        column="other_type"
	
    />

    <many-to-one name="xzfyInfo" cascade="all" 
			column="xzfy_id" class="XzfyInfo"  />
	
</class>
</hibernate-mapping>
