<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
<hibernate-mapping package="com.taiji.fzb.domain">
<class 
    name="Member" 
    table="member"
    dynamic-update="true"
	dynamic-insert="true"    
>
    <id
        name="memId"
        type="java.lang.Integer"
        column="mem_id"
    >
        <generator class="increment" />
    </id>

   <property
        name="memName"
        type="java.lang.String"
        column="mem_name"
      
    />	
    
    <property
        name="sexy"
        type="java.lang.String"
        column="sexy"

    />
    <property
        name="workName"
        type="java.lang.String"
        column="work_name"

    />
   <property
        name="phone"
        type="java.lang.String"
        column="phone"
 
    />	
    <property
        name="techType"
        type="java.lang.String"
        column="tech_type"
 
    />
    <property
        name="education"
        type="java.lang.String"
        column="edu"
 
    />
    <property
        name="email"
        type="java.lang.String"
        column="email"
 
    />
    <property
        name="certId"
        type="java.lang.String"
        column="cert_id"
 
    />
    <property
        name="born"
        type="java.lang.String"
        column="born"
 
    />
    <property
        name="age"
        type="java.lang.String"
        column="age"
 
    />
			
</class>
</hibernate-mapping>
