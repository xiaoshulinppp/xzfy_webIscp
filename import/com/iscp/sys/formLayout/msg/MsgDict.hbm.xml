<?xml version="1.0"?><!DOCTYPE hibernate-mapping PUBLIC	"-//Hibernate/Hibernate Mapping DTD 2.0//EN"	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.iscp.sys.formLayout.msg">
<class name="MsgDict" table="t_dict">
	<id name="id" type="integer" column="dict_id">
		<generator class="increment"/>
	</id>
	<property name="name" type="java.lang.String" column="dict_name" length="50"/>
	<property name="abbr" type="java.lang.String" column="dict_abbr" length="50"/>
</class>
</hibernate-mapping>
