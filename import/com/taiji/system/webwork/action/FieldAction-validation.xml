<?xml version="1.0" encoding="gb2312"?>
<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0//EN" "http://www.opensymphony.com/xwork/xwork-validator-1.0.dtd">
<validators>
    <field name="field.name">
        <field-validator type="requiredstring">
			<message key="请输入字段名"/>
        </field-validator>
    </field>
	
    <field name="field.showName">
        <field-validator type="requiredstring">
			<message key="请输入字段显示名称"/>
        </field-validator>
    </field>
     <field name="field.type">
        <field-validator type="requiredstring">
			<message key="请输入字段的数据类型"/>
        </field-validator>
    </field>
	
    <field name="field.showType">
        <field-validator type="requiredstring">
			<message key="请输入字段显示类型"/>
        </field-validator>
    </field>
    <field name="field.order">
        <field-validator type="number">
			<message key="字段长度必须为数字"/>
        </field-validator>
    </field>
     <field name="field.length">
        <field-validator type="number">
			<message key="字段长度必须为数字"/>
        </field-validator>
    </field>
</validators>